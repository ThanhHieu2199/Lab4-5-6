@Model IEnumerable<Lab456.Models.Course>
@{
    ViewBag.Title = "Home Page";
}



<div class="row">
    <ul>
        @foreach (var course in Model)
        {
            <li>
                @course.DateTime - @course.Lecturer.Name
            </li>
        }
    </ul>
   

</div>
<ul class="course">
    @foreach( var course in Model)
    {
        <li>
            <div class="date">
                <div class="month">
                    @course.DataTime.Tostring("MM")
                </div>
                <div class="day">
                    @course.DataTime.Tostring("MM")
                </div>
            </div>
        </li>
    }
</ul>